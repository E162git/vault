{{log model.connections.length "MODEl"}}
<div class="box is-fullwidth is-shadowless has-tall-padding">
  <div class="selectable-card-container">
    <SelectableCard
      @cardTitle="Connections"
      @total="100"
      @subText="The total number of connections to external databases that you have access to."
      @actionCard={{true}}
      @actionText="Configure new"
      @actionTo={{vault.cluster.secrets.backend.list-root}}
      @queryParam={{'connection'}}
    />
    <SelectableCard
      @cardTitle="Roles"
      @total="1001"
      @subText="The total number of roles that have been set up, both dynamic and static."
      @actionCard={{true}}
      @actionText="Create new"
      @actionTo={{vault.cluster.secrets.backend.list-root}}
      @queryParam={{'role'}}
    />
    <SelectableCard
      @cardTitle="Active direct tokens"
      @total="1002"
      @subText="Current namespace"
      @actionCard={{true}}
      @isSearchSelect={{true}}
      @searchText="Get Credentials"
      {{!-- This is gonna have like a button and search field but not action Text --}}
    />
  </div>
</div>
